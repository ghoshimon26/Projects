/*Apple - best predictions to buy and sell*/

select date, close
from apple2
where `signal` = &#39;Buy&#39;
order by close
limit 1;

select date, close
from apple2
where `signal` = &#39;Sell&#39;
order by close desc
limit 1;

/*Amazon - best predictions to buy and sell*/
select date, close
from amazon2
where `signal` = &#39;Buy&#39;
order by close
limit 1;

select date, close
from amazon2
where `signal` = &#39;Sell&#39;
order by close desc
limit 1;

