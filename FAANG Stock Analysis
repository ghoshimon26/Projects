/*Calculating the % returns for FAANG from 2020-01-02 to 2021-03-26*/
select round((max(close) - min(open)) / min(open) * 100, 2) AS
percentage_gain
from amazon
where NewDate in (date&#39;2020-01-02&#39;, date&#39;2021-03-26&#39;);

select round((max(close) - min(open)) / min(open) * 100, 2) AS
percentage_gain
from apple
where NewDate in (date&#39;2020-01-02&#39;, date&#39;2021-03-26&#39;);

select round((max(close) - min(open)) / min(open) * 100, 2) AS
percentage_gain
from facebook
where NewDate in (date&#39;2020-01-02&#39;, date&#39;2021-03-26&#39;);

select round((max(close) - min(open)) / min(open) * 100, 2) AS
percentage_gain
from google
where NewDate in (date&#39;2020-01-02&#39;, date&#39;2021-03-26&#39;);

select round((max(close) - min(open)) / min(open) * 100, 2) AS
percentage_gain
from netflix
where NewDate in (date&#39;2020-01-02&#39;, date&#39;2021-03-26&#39;);
